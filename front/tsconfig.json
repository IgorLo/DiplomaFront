{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "dom",
      "es2015.iterable"
    ],
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/local/tsc.js",
    "sourceMap": true,
    "moduleResolution": "node",
    "declaration": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}